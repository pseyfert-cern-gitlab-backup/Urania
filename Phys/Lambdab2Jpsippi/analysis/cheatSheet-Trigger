//===================================================================
TFile *_file0 = TFile::Open("Lambdab-R12S17b-721-"+NN+"-SW.root")
TTree* SW = (TTree*)_file0->Get("WeightTree")
TFile* f1 = TFile::Open("/home/pkoppenb/work/Baryons/Lambdab/Lambdab-R12S17b-721.root")

TTree* T = f1->Get("BetaSLambdab2JpsippiDetachedLine_Tuple/DecayTree")
T->AddFriend(SW)
T->Draw("B_FullFit_M:wPsippiMass","runNumber==102182 && wCategory==1")
//===================================================================
