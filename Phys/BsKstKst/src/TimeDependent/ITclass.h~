class ITclass {

  public:

  Double_t IT_cosh(Int_t year_opt, Int_t trig_opt, Int_t wide_window) {
    if ((year_opt == 0) && (trig_opt == 0) && (wide_window == 0)) {return '+str(IT_cosh_accu_2011_L0TIS_narrow.getVal())+';}
    else if ((year_opt == 0) && (trig_opt == 0) && (wide_window == 1)) {return '+str(IT_cosh_accu_2011_L0TIS_wide.getVal())+';}
    else if ((year_opt == 0) && (trig_opt == 1) && (wide_window == 0)) {return '+str(IT_cosh_accu_2011_L0noTIS_narrow.getVal())+';}
    else if ((year_opt == 0) && (trig_opt == 1) && (wide_window == 1)) {return '+str(IT_cosh_accu_2011_L0noTIS_wide.getVal())+';}
    else if ((year_opt == 1) && (trig_opt == 0) && (wide_window == 0)) {return '+str(IT_cosh_accu_2012_L0TIS_narrow.getVal())+';}
    else if ((year_opt == 1) && (trig_opt == 0) && (wide_window == 1)) {return '+str(IT_cosh_accu_2012_L0TIS_wide.getVal())+';}
    else if ((year_opt == 1) && (trig_opt == 1) && (wide_window == 0)) {return '+str(IT_cosh_accu_2012_L0noTIS_narrow.getVal())+';}
    else if ((year_opt == 1) && (trig_opt == 1) && (wide_window == 1)) {return '+str(IT_cosh_accu_2012_L0noTIS_wide.getVal())+';}
    return 0.;
    };



};
